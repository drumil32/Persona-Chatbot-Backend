# Server Configuration
PORT=3000
NODE_ENV=development

# API Keys
GEMINI_API_KEY=your_gemini_api_key
OPENAI_API_KEY=your_openai_api_key
DEEPSEEK_API_KEY=your_deepseek_api_key
CLAUDE_API_KEY=your_claude_api_key
GROQ_API_KEY=your_groq_api_key

# API Base URLs (optional - defaults provided)
GEMINI_BASE_URL=https://generativelanguage.googleapis.com/v1beta
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
CLAUDE_BASE_URL=https://api.anthropic.com/v1
GROQ_BASE_URL=https://api.groq.com/openai/v1

# CORS Configuration - Frontend URLs
FRONTEND_URL_1=http://localhost:3000
FRONTEND_URL_2=http://localhost:3001

# Security Configuration - Allowed Origins (can be same as frontend URLs or different)
ALLOWED_ORIGIN_1=http://localhost:3000
ALLOWED_ORIGIN_2=http://localhost:3001

# Logging Level (optional)
LOG_LEVEL=info

# Rate Limiting Configuration
# Default rate limits (fallback for all models)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=10

# Model-specific rate limits (optional - falls back to default if not set)
# Gemini 2.0 Flash - Higher limit due to fast response
RATE_LIMIT_GEMINI_WINDOW_MS=60000
RATE_LIMIT_GEMINI_MAX_REQUESTS=15

# GPT-4o Mini - Lower limit due to cost considerations
RATE_LIMIT_GPT_WINDOW_MS=60000
RATE_LIMIT_GPT_MAX_REQUESTS=8

# Deepseek - Moderate limit
RATE_LIMIT_DEEPSEEK_WINDOW_MS=60000
RATE_LIMIT_DEEPSEEK_MAX_REQUESTS=12

# Claude - Strict limit due to API constraints
RATE_LIMIT_CLAUDE_WINDOW_MS=60000
RATE_LIMIT_CLAUDE_MAX_REQUESTS=5

# Groq Llama - Higher limit due to speed
RATE_LIMIT_GROQ_WINDOW_MS=60000
RATE_LIMIT_GROQ_MAX_REQUESTS=20

# Global rate limiting settings
RATE_LIMIT_SKIP_SUCCESSFUL=false
RATE_LIMIT_SKIP_FAILED=false